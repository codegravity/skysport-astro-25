---
import { Image } from 'astro:assets';
import logo from '@assets/images/Skysport_Logo-01-invert-468.png';
---

<a href="/" >
    <Image 
        src={logo} 
        alt="Skysport Home" 
        class="  w-2/3 lg:w-3/4  hs-auto object-contain " 
        width={468}
        height={72}
        decoding="async"
        loading="lazy"
        format="webp"
       
    />
</a>
