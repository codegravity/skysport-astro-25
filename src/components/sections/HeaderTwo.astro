---
//import { Moon, SunMedium} from "@lucide/astro";
//import menus from "@/collections/menu.json";


import WeatherMenu from "@collections/menuWeather.json";

import Logo from "@elements/LogoCG.astro";
import Kurser from "@components/sections/KursMenu.astro";
import Weather from "@components/sections/WeatherMenu.astro";
import MenuTest from "@components/sections/menutest.astro";
//import HeaderEnd from "@/components/sections/HeaderEnd.astro";


const WeatherContent = {
    title: "VÄDER",
    description: "Skärmflygutbildning, Butik, Resor och Tandemflygning från Åreskutan.",
    weatherLinks: WeatherMenu,
    externalLinkIcon: "external-link",
}
---

<div class="relative w-full h-16 sm:h-20 opacity-0 pointer-events-none"></div>
<header id="header" class="fixed top-0 sm:top-1 z-50 w-full px-1 sm:px-4 lg:px-6 flex flex-wrap sm:justify-start sm:flex-nowrap z-50 w-full bg-white text-sm py-1 sm:py-4 dark:bg-neutral-900">
  <nav class="max-w-[85rem] w-full mx-auto px-4 md:px-6 lg:px-8">
    <div class="relative sm:flex sm:items-center">
      <div class="flex items-center justify-between"> 
         <Logo />
        <div class="sm:hidden">
          <button type="button" class="hs-collapse-toggle p-2 inline-flex justify-center items-center gap-x-2 rounded-lg border border-gray-200 bg-white text-gray-800 shadow-2xs hover:bg-gray-50 focus:outline-hidden focus:bg-gray-50 disabled:opacity-50 disabled:pointer-events-none dark:bg-transparent dark:border-neutral-700 dark:text-white dark:hover:bg-white/10 dark:focus:bg-white/10" id="hs-navbar-basic-usage-collapse" aria-expanded="false" aria-controls="hs-navbar-basic-usage" aria-label="Toggle navigation" data-hs-collapse="#hs-navbar-basic-usage">
            <svg class="hs-collapse-open:hidden shrink-0 size-4" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><line x1="3" x2="21" y1="6" y2="6"/><line x1="3" x2="21" y1="12" y2="12"/><line x1="3" x2="21" y1="18" y2="18"/></svg>
            <svg class="hs-collapse-open:block hidden shrink-0 size-4" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M18 6 6 18"/><path d="m6 6 12 12"/></svg>
          </button>
        </div>
      </div>

      <div id="hs-navbar" class="hidden hs-collapse overflow-hidden transition-all duration-300 basis-full grow sm:block" aria-labelledby="hs-navbar-basic-usage-collapse">
        <div class="flex flex-col gap-y-3 gap-x-6 sm:gap-y-0 mt-5 sm:flex-row sm:items-center sm:justify-end sm:mt-0 sm:ps-5">
          <!-- menu links -->
          <a class="sm:p-2 font-medium text-blue-500 focus:outline-hidden" href="/" aria-current="page">HEM</a>
          <a class="font-medium text-gray-600 hover:text-gray-400 focus:outline-hidden focus:text-gray-400 dark:text-neutral-400 dark:hover:text-neutral-500 dark:focus:text-neutral-500" href="/tandem">TANDEM</a>
          <!-- <Kurser />
          <Weather content={WeatherContent}/>  -->
          <MenuTest />
          <a class="font-medium text-gray-600 hover:text-gray-400 focus:outline-hidden focus:text-gray-400 dark:text-neutral-400 dark:hover:text-neutral-500 dark:focus:text-neutral-500" href="#">RESOR</a>
          <a class="font-medium text-gray-600 hover:text-gray-400 focus:outline-hidden focus:text-gray-400 dark:text-neutral-400 dark:hover:text-neutral-500 dark:focus:text-neutral-500" href="#">BOENDE</a>
          <a class="font-medium text-gray-600 hover:text-gray-400 focus:outline-hidden focus:text-gray-400 dark:text-neutral-400 dark:hover:text-neutral-500 dark:focus:text-neutral-500" href="#">OM OSS</a>
        
         <!-- end menu links -->
        </div>
      </div>
    
     <!--  <HeaderEnd /> -->

    </div>
  </nav>
</header>



  

