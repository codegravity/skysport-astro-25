import { Astronav, MenuItems, MenuIcon, Dropdown, DropdownItems, DropdownSubmenu } from "astro-navbar";
import { headerMenu } from '@data/menu';
import { Icon } from 'astro-icon/components';
import Logo from "@components/elements/LogoCG.astro";
import NavSuffix from '@elements/bannersuffix.astro'



const yeary = "2026";
// get the page URL
const {pageURL} = Astro.props;
// Get the current path to determine active menu item
const currentPath = Astro.url.pathname;

// Function to check if a link is the current page or a parent of the current page
const isCurrentPage = (link: string) => {
    if (link === '/') {
        return currentPath === '/';
    }
    return currentPath.startsWith(link);
};
